SELECT TaskId,
       processInstanceId,
       mailFrom,
       mailTo,
       mailCC,
       mailStatus,
       successTime,
       insertedTime,
       mailSubject,
       statusComments,
       mailMessage
FROM WFMAILQUEUEHISTORYTABLE
WHERE CAST(successTime AS DATE) = CAST(GETDATE() AS DATE)
  AND CAST(successTime AS TIME) >= '12:00:00'
ORDER BY successTime DESC;
